# RIAWELC Environment Variables

# MLflow
MLFLOW_TRACKING_URI=mlruns

# API
RIAWELC_MODEL_PATH=outputs/models/checkpoints/efficientnetb0/v1/fine_tune/best.keras
RIAWELC_HOST=0.0.0.0
RIAWELC_PORT=8000
RIAWELC_LOG_LEVEL=info
RIAWELC_CORS_ORIGINS=http://localhost:1420,http://localhost:8000,http://localhost:5173
RIAWELC_API_KEY=
RIAWELC_RATE_LIMIT=30

# U-Net segmentation model paths
RIAWELC_SEG_BASELINE_PATH=outputs/models/checkpoints/unet_efficientnetb0_v2/v2/best.keras
RIAWELC_SEG_AUGMENTED_PATH=outputs/models/checkpoints/unet_efficientnetb0_augmented/v1/best.keras

# OpenTelemetry (optional)
OTEL_ENABLED=false
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_SERVICE_NAME=riawelc-api

# Vertex AI
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=europe-west3
GCP_SERVICE_ACCOUNT=YOUR_PROJECT_NUMBER-compute@developer.gserviceaccount.com
GCS_DATA_BUCKET=your-data-bucket
GCS_ARTIFACTS_BUCKET=your-artifacts-bucket
VERTEX_CONTAINER_URI=europe-west3-docker.pkg.dev/your-gcp-project-id/training/riawelc-training:latest

# Shell script bucket aliases (same values as above)
RIAWELC_DATA_BUCKET=your-data-bucket
RIAWELC_ARTIFACTS_BUCKET=your-artifacts-bucket
